// prisma/schema.prisma

generator client {  
  provider = "prisma-client-js" 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  
} 

model Student {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model User {
  id            String @id @default(cuid())
  name          String
  email         String @unique
  createdAt     DateTime @default(now())
  updatedAt     DateTime @default(now()) @updatedAt
}